<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated search_result -->
      <package
            uri="http://data-in-motion.biz/search/result/1.0"
            class="org.gecko.searchresult.SearchResultPackage"
            genModel="model/search_result.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.content_parser">
      <!-- @generated search_result -->
      <parser
            contentTypeIdentifier="searchresult#1.0"
            class="org.gecko.searchresult.util.SearchResultResourceFactoryImpl"/>
   </extension>

   <extension point="org.eclipse.core.contenttype.contentTypes">
      <!-- @generated search_result -->
      <content-type
            base-type="org.eclipse.emf.ecore.xmi"
            file-extensions="searchresult"
            id="searchresult#1.0"
            name="%_UI_SearchResult_content_type"
            priority="normal">
         <describer class="org.eclipse.emf.ecore.xmi.impl.RootXMLContentHandlerImpl$Describer">
            <parameter name="namespace" value="http://data-in-motion.biz/search/result/1.0"/>
            <parameter name="kind" value="xmi"/>
         </describer>
      </content-type>
   </extension>

</plugin>
