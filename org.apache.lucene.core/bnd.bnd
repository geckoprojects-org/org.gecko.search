-buildpath: org.apache.lucene:lucene-core;version=latest,\
	org.apache.lucene:lucene-analyzers-common;version=latest


Bundle-Version: 8.4.1.SNAPSHOT

Export-Package: *

-includeresource: \
	@${repo;org.apache.lucene:lucene-analyzers-common;latest}!/META-INF/services/*,\
	OSGI-OPT/src=@${repo;org.apache.lucene:lucene-analyzers-common:jar:sources},\
	@${repo;org.apache.lucene:lucene-core;latest}!/META-INF/services/*,\
	OSGI-OPT/src=@${repo;org.apache.lucene:lucene-core:jar:sources}